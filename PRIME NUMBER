using namespace std;
int main()
{
	int n,i;
	bool isprime=true;
	cout<<"value of n "<<endl;
	cin>>n;
	if(n<1)
	{
		isprime=false;
	}
	else{
		for(i=2;i<n/2;i++)
		{
			if(n%i==0)
			{
				isprime=false;
			}
		}
	}
if(isprime)
{
	cout<<"the given number is prime"<<endl;
	
}
else
{
	cout<<"the given number is not prime"<<endl;
}
return 0;
}
